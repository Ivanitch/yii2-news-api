{
    "name": "ivanitch/yii2-api-news",
    "description": "Api application news feed on php framework Yii2",
    "keywords": ["yii2", "api"],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "require": {
        "php": ">=7.4.5",
        "fishvision/yii2-migrate": "*",
        "kartik-v/yii2-field-range": "dev-master",
        "kartik-v/yii2-widget-datepicker": "^1.4",
        "la-haute-societe/yii2-save-relations-behavior": "*",
        "mihaildev/yii2-elfinder": "*",
        "paulzi/yii2-nested-sets": "^1.0",
        "webmozart/assert": "^1.2",
        "yiisoft/yii2": "~2.0.18",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
